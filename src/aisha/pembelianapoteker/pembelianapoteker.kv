<TransacButton@Button>:
    background_color: (0,0,0,0)
    canvas.before:
        Color:
            rgba: rgba('#d9d9d9') if self.state=='normal' else rgba('#ffbb73')
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [10]

