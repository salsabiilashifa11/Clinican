#======WINDOW PEMBELIAN APOTEKER==========        
<PembelianapotekerWindow>:
#----BACKGROUND
    padding: 20,20,20,20
    spacing: 12
    background_color: rgba('#FFFFFF')
    orientation: 'vertical'
    canvas:
        Color:
            rgba: rgba('#f0f0f0')
        Rectangle:
            size: self.size
            pos: self.pos

#-----HEADER
    BoxLayout:
        size_hint_y: 0.2
        orientation: 'horizontal'
        spacing: 20
        canvas:
            Color:
                rgba: rgba('#ffae70')
            RoundedRectangle:
                size: self.size
                pos: self.pos
                radius: [17, 17, 0, 0]
        padding: [50,10,50,10]
        #profile
        Button:
            background_color: (0,0,0,0)
            size_hint_x: 0.1
            canvas.before:
                Color:
                    rgba: rgba('#ffae70') if self.state=='normal' else rgba('#d9d9d9')
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [10]
                    source: '../img/leftarrow4.png'
            on_release: root.to_homeapoteker()
        BoxLayout:
            size_hint_x: 0.15            
            canvas:
                Rectangle:
                    size: (100,100)
                    pos: (self.pos[0], self.pos[1]-10)
                    source: '../img/profile.png'
        Label:
            id: name_field
            text: 'ADMIN'
            color: rgba('#FFFFFF')
            text_size: self.size
            halign: 'left'
            valign: 'center'
            padding_x: 30 
            bold: True
            font_size: 50

#-----KONTEN
    BoxLayout:
        orientation: 'horizontal'
        spacing: 30
        canvas:
            Color:
                rgba: rgba('#F0F0F0')
            RoundedRectangle:
                size: self.size
                pos: self.pos
                radius: [0,0,17,17]
#---------DAFTAR TRANSAKSI PEMBELIAN
        BoxLayout:
            canvas:
                Color:
                    rgba: rgba('#e3e3e3')
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [0,0,17,17]
            orientation: 'vertical'
            size_hint_x: 0.8
            spacing: 12
            padding: 50,20,50,50
            Label:
                size_hint_y: 0.1
                text_size: self.size
                halign: 'left'
                valign: 'center'
                text: 'DAFTAR PEMBELIAN'
                color: rgba('#000000')
                bold: True
                font_size: 20
            TransacButton:
                text: 'TRANSAKSI 1'
                font_size: 15
                size_hint_y: 0.07
                on_release: root.showCekBeli(1)
                color: rgba('#000000')
            TransacButton:
                text: 'TRANSAKSI 2'
                font_size: 15
                size_hint_y: 0.07
                on_release: root.showCekBeli(2)
                color: rgba('#000000')
            TransacButton:
                size_hint_y: 0.07
                text: 'TRANSAKSI 3'
                font_size: 15
                on_release: root.showCekBeli(3)
                color: rgba('#000000')
            TransacButton:
                size_hint_y: 0.07
                text: 'TRANSAKSI 4'
                font_size: 15
                on_release: root.showCekBeli(4)
                color: rgba('#000000')
            TransacButton:
                size_hint_y: 0.07
                text: 'TRANSAKSI 5'
                font_size: 15
                on_release: root.showCekBeli(5)
                color: rgba('#000000')
            TransacButton:
                size_hint_y: 0.07
                text: 'TRANSAKSI 6'
                font_size: 15
                on_release: root.showCekBeli(6)
                color: rgba('#000000')
            TransacButton:
                size_hint_y: 0.07
                text: 'TRANSAKSI 7'
