        Rectangle:
            size: self.size
            pos: self.pos

#------------------------------------HEADER--------------------------------------------------
    BoxLayout:
        size_hint_y: 0.2
        canvas:
            Color:
                rgba: rgba('#F0F0F0')
            RoundedRectangle:
                size: self.size
                pos: self.pos
                radius: (10,10,10,10)
        
#-------------BACK BUTTON
        BoxLayout:
            size_hint_x: 0.1
            padding: (30,30,30,30)
            MyButton2:
                canvas:
                    Rectangle:
                        size: self.size
                        pos: self.pos
                        source: '../img/leftarrow5.png'
                on_release: root.to_homeuser()

#-------------JUDUL
        Label:
            size_hint_x: 0.7
            text: "Pembelian Obat"
            #bold: True
            hallign: 'center'
            vallign: 'bottom'
            font_size: 40
            color: rgba('#6f6f6f')

#-------------INFO PEMBELIAN
