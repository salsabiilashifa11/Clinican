#BUTTON CUSTOM 1
<MyButton@Button>:
    background_color: (0,0,0,0)
    canvas.before:
        Color:
            rgba: rgba('#f0e267') if self.state=='normal' else rgba('#53CBFF')
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: (17,17,17,17)

#BUTTON CUSTOM 2
<MyButton2@Button>:
    background_color: (0,0,0,0)
    canvas.before:
        Color:
            rgba: rgba('#F0F0F0') if self.state=='normal' else rgba('#53CBFF')
        Rectangle:
            pos: self.pos
            size: self.size 

#BUTTON CUSTOM3
<MyButton3@Button>:
    background_color: (0,0,0,0)
    canvas.before:
        Color:
            rgba: rgba('#C4C4C4') if self.state=='normal' else rgba('#53CBFF')
        Rectangle:
            pos: self.pos
            size: self.size 


#------------PESAN STOK TIDAK CUKUP
<EmptyStock>:
    orientation: 'vertical'
    canvas:
        Color:
            rgba: rgba('#f0f0f0')
        Rectangle:
            size: self.size
            pos: self.pos
    Label:
        size_hint_y: 0.7
        text: "Stok tidak cukup!"
        font_size: 15
        color: rgba('#6f6f6f')
        pos_hint: {"x":0, "top":1}

    BoxLayout:
        pos_hint: {"x":0, "y":0}
        size_hint_y: 0.3
        padding: (5,5,5,5)
        Label:
            size_hint_x: 0.3
        MyButton3:
            size_hint_x: 0.2
            text: "Tutup"
            font_size: 15
            color: rgba('#6f6f6f')
            on_release: root.closePopup()
        Label:
            size_hint_x: 0.3
        

#------------PESAN PEMBELIAN TIDAK ADA
<EmptyKeranjang>:
    orientation: 'vertical'
    canvas:
        Color:
            rgba: rgba('#F0F0F0')
        Rectangle:
            size: self.size
            pos: self.pos
    Label:
        pos_hint: {"x":0, "top":1}
        size_hint_y: 0.7
        text: root.text
        font_size: 15
        color: rgba('#6f6f6f')
        
    BoxLayout:
        pos_hint: {"x":0, "y":0}
        size_hint_y: 0.3
        padding: (5,5,5,5)
        Label:
            size_hint_x: 0.3
        MyButton3:
            size_hint_x: 0.2
            text: "Tutup"
            font_size: 15
            color: rgba('#6f6f6f')
            on_release: root.closePopup()
        Label:
            size_hint_x: 0.3

#------------KONFIRMASI KE PEMBAYARAN
<Confirmation>:
    canvas:
        Color:
            rgba: rgba('#F0F0F0')
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        pos_hint: {"x":0, "top":1}
        orientation: 'vertical'
        Label:
            size_hint_y: 0.5
            text: "Lanjut ke pembayaran?"
            font_size: 15
            color: rgba('#6f6f6f')

        BoxLayout:
            padding: (5,5,5,5)
            size_hint_y: 0.5
            Label:
                size_hint_x:0.1
            MyButton3:
                size_hint_x: 0.35
                text: "Batal"
                font_size: 15
                color: rgba('#6f6f6f')
                on_release: root.closePopup()
            Label:
                size_hint_x: 0.1
            MyButton3:
                size_hint_x: 0.35
                text: "Konfirmasi"
                font_size: 15
                color: rgba('#6f6f6f')
                on_release: root.to_payment()
                on_release: root.closePopup()
            Label:
                size_hint_x: 0.1
            

#----------------INFO PEMBELIAN
<PopupInfo>:
    orientation: 'vertical'
    canvas:
        Color:
            rgba: rgba('#F0F0F0')
        Rectangle:
            size: self.size
            pos: self.pos
    Label:
        size_hint_y: 0.85
        text: root.receipt
        font_size: 16
        color: rgba('#6f6f6f')
        pos_hint: {"x":0, "top":1}

    BoxLayout: 
        pos_hint: {"x":0, "y":0}
        size_hint_y: 0.15
        padding: (5,5,5,5)
        Label:
            size_hint_x: 0.4
        MyButton3:
            size_hint_x: 0.3
            text: "Tutup"
            font_size: 15
            color: rgba('#6f6f6f')
            on_release: root.closePopup()
        Label:
            size_hint_x: 0.4



#--------------MENAMPILKAN DEKSRIPSI OBAT
<PopupObat>:
    orientation : 'vertical'
    canvas:
        Color:
            rgba: rgba('#F0F0F0')
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        padding: (50,30,50,30)
        size_hint_y: 0.3
        pos_hint: {"x":0.25, "top":1}
        canvas:
            Rectangle:
                id: obat_pic
                size: (self.size[0]/2.2, self.size[1]/1.2)
                pos: self.pos
                source: root.pic
    Label:
        id: obat_desc
        size_hint_y: 0.6
        text: root.desc
        font_size: 13
        color: rgba('#6f6f6f')
        pos_hint: {"x":0, "top":0.7}
    BoxLayout:
        padding: (5,5,5,5)
        background_color: rgba('#c4c4c4')
        pos_hint: {"x":0, "y":0}
        size_hint_y: 0.1
        Label:
            size_hint_x: 0.35
        MyButton3:
            size_hint_x: 0.3
            text: "Tutup"
            font_size: 15
            color: rgba('#6f6f6f')
            on_release: root.closePopup()
        Label:
            size_hint_x: 0.35
        



